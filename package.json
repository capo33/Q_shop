{
  "name": "q-shop",
  "version": "1.0.0",
  "description": "eCommerce app built with mern stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "serve": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
